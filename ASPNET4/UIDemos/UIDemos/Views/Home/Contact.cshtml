@using UIDemos.Helpers
@model UIDemos.Models.Contact
@{
	ViewBag.Title = "Contact Page";
}

<br /><br /><br />

@using (Html.BeginPanel("Contact"))
{
    using (Html.BeginForm())
    {
	    @Html.EditorFor(m => m)

        using (Html.BeginRow())
        {
		    <div class="col-md-12">
			    <button type="submit" class="btn btn-primary">Submit</button>
		    </div>
        }
    }
}

@section Scripts {
	@Scripts.Render("~/bundles/jqueryval")
}
